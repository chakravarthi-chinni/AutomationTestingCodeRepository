package com.chax.psl;

import java.util.Scanner;

public class ChocoloateFactory 
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("enter n value of chocolates : ");
		int candy=sc.nextInt();
		Double[] Array=new Double[candy];
		System.out.println("chocolate list is : ");
		for(int i=0;i<candy;i++)
		{
			Array[i]=sc.nextDouble();
		}
	}

}
