package com.chax.practice;

public class Raju extends Chax{
	int a,b,c;
	
	public Raju() {
		this.a=super.a;
		this.b=super.b;
		this.c=super.c;
	}
	public void raju() {
		System.out.println("im raju and i dont have any problem");
		int add=a+b+c;
		System.out.println("raju addition of a,b,c is :"+add);
	}

}
