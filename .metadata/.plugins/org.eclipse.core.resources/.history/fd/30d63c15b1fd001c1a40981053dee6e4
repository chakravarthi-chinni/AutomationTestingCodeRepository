package com.chax.psl;
import java.util.*;

public class DuplicateLettersShow 
{
	
	public static void main(String[] args) 
	{
		int i;
		
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the String : ");
		String s1=sc.nextLine();
		String s2=s1;
		for(i=0;i<s1.length();i++)
		{
			
			for(i=0;i<s1.length();i++)
			{
				char ch=s1.charAt(i);
			if(s2.equals(ch))
			{
				System.out.print(ch);
			}
			
			}
			
		}
		
	}
}
